{
  "name": "@superblocksteam/react-diff-viewer",
  "version": "3.3.0",
  "private": false,
  "description": "A simple and beautiful text diff viewer component made with diff and React",
  "keywords": [
    "review",
    "code-review",
    "diff",
    "diff-viewer",
    "github",
    "react",
    "react-component",
    "ui"
  ],
  "repository": "https://github.com/superblocksteam/react-diff-viewer.git",
  "license": "MIT",
  "main": "lib/index",
  "typings": "lib/index",
  "scripts": {
    "build": "tsc --outDir lib/",
    "build:examples": "webpack --progress --colors",
    "build:watch": "tsc --outDir lib/ -w",
    "publish:examples": "NODE_ENV=production yarn build:examples && gh-pages -d examples/dist -r $GITHUB_REPO_URL",
    "publish:examples:local": "NODE_ENV=production yarn build:examples && gh-pages -d examples/dist",
    "start:examples": "webpack-dev-server --open --hot --inline",
    "test": "mocha --require ts-node/register --require enzyme.ts ./test/**",
    "test:watch": "mocha --require ts-node/register --require enzyme.ts --watch-extensions ts,tsx --watch ./test/**"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "create-emotion": "^10.0.27",
    "diff": "^5.1.0",
    "emotion": "^10.0.27",
    "memoize-one": "^5.2.1",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@types/diff": "^5.0.3",
    "@types/enzyme": "^3.10.12",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/expect": "^1.20.4",
    "@types/memoize-one": "^4.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.20.55",
    "@types/react": "^16.14.39",
    "@types/react-dom": "^16.9.18",
    "@types/webpack": "^4.41.33",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "expect": "^24.9.0",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^4.3.0",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "1.2.1",
    "mocha": "^6.2.3",
    "node-sass": "^7.0.3",
    "raw-loader": "^3.1.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "sass-loader": "^10.2.0",
    "spy": "^1.0.0",
    "ts-loader": "^8.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3"
  },
  "peerDependencies": {
    "react": "^15.3.0 || ^16.0.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^15.3.0 || ^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "engines": {
    "node": ">= 14"
  }
}
